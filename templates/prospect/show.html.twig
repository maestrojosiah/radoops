{% extends 'base.html.twig' %}

{% block body %}
    <h1>Prospect</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prospect.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ prospect.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ prospect.email }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ prospect.phone }}</td>
            </tr>
            <tr>
                <th>Residence</th>
                <td>{{ prospect.residence }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('prospect_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
